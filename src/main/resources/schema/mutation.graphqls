type Mutation {
	createContact(request: CreateContactRequest) : ContactResponse
}

input CreateContactRequest {
	firstName: String
	lastName: String
	birthDate: String
	email: String
	address: CreateAddressRequest
	phones: [CreatePhoneRequest]
}

input CreateAddressRequest {
	street: String!
	suburb: String!
	state: String!
	postCode: String
}

input CreatePhoneRequest {
	phoneType: String
	phoneNumber: String
}
